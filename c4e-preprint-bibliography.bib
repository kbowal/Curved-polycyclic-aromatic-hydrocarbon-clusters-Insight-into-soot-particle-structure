author = {Filatov, Alexander S. and Petrukhina, Marina A.},
doi = {10.1016/j.ccr.2010.05.004},
journal = {Coordination Chemistry Reviews},
keywords = {Buckybowls,Fullerene fragments,Metal complexation,Metal-??-arene interactions,Open geodesic polyarenes},
mendeley-groups = {Carbon/Soot/Soot nucleation/Ion nucleation,Carbon/Soot/Soot reactivity},
number = {17-18},
pages = {2234--2246},
title = {{Probing the binding sites and coordination limits of buckybowls in a solvent-free environment: Experimental and theoretical assessment}},
