@article{DiStasio2011,
abstract = {Small-angle X-ray scattering (SAXS) has been used in a comparative study$\backslash$nto characterize soot particle formation in a partially premixed$\backslash$nethylene-air flame into which plain water, and 0.1 mol/L solutions of$\backslash$npotassium chloride, cesium chloride, barium chloride, and calcium$\backslash$nchloride were aspirated. The plain water results are assumed to be the$\backslash$nreference in this study. On the basis of previous findings, a two-level$\backslash$nuniversal fitting function for the scattering intensity I(q), 0.04$\backslash$nnm(-1) {\textless} q {\textless} 0.5 nm(-1), is used to retrieve the size and concentration$\backslash$nof both primary particles, which are the subunits of relatively large$\backslash$n(similar to 100 nm) fractal aggregates, and smaller subprimary$\backslash$nparticles, which aggregate in turn to form primaries. It is found that$\backslash$nCsCl addition produces similar to 40{\%} smaller primary particles at$\backslash$nlarger residence times, compared to plain water addition. CaCl2, BaCl2,$\backslash$nand KCl addition have almost no effect on primary and subprimary sizes.$\backslash$nCesium chloride addition produces a dramatic reduction of the subprimary$\backslash$nparticle dimension, up to a factor of 2.2, while the concentration of$\backslash$nprimaries and subprimaries is increased by factors of up to 5 and 2,$\backslash$nrespectively, with respect to plain water. The contribution to the soot$\backslash$nvolume fraction (fv) from primary and subprimary particles is separately$\backslash$nestimated for all the additives, with respect to the standard plain$\backslash$nwater addition. At larger residence times, CsCl addition leads to a$\backslash$nreduction of the fv contribution from primaries, with larger rates$\backslash$ncompared to KCl. On the other hand, the effect of suppression by CsCl at$\backslash$nsmaller heights can be weaker than pure water, contrary to KCl, which is$\backslash$na better fv suppressor than water at all heights, even if a smaller rate$\backslash$nthan that for CsCl is in operation. The effect of CsCl and KCl is the$\backslash$nsame for the subprimary fv contribution. BaCl2 has the effect of$\backslash$nyielding a primary fv contribution, which is a factor of 2 smaller than$\backslash$nthat for plain water, almost independently of the height in the flame$\backslash$nand, moreover, it yields a large decrease (up to a factor of 4 at a$\backslash$nheight above the burner (HAB) of 28 mm) in the subprimary fv$\backslash$ncontribution, compared to plain water. CaCl2 addition has a negligible$\backslash$neffect on both the primary and subprimary fv contribution, with respect$\backslash$nto water.},
author = {{Di Stasio}, S. and Legarrec, J. L. and Mitchell, J. B.A.},
doi = {10.1021/ef1012209},
journal = {Energy and Fuels},
number = {3},
pages = {916--925},
